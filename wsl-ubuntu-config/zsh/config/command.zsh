# push/pull Documents
alias pd='zsh ~/.zsh/script/push_documents.sh'
alias pp='zsh ~/.zsh/script/pull_documents.sh'

# easy push
alias UpdateRepo="~/.zsh/script/update_repo.sh"

alias SetGoPath="source ~/.zsh/script/set_go_path.sh"
alias CreateGoProject="zsh ~/.zsh/script/create_go_project.sh"

alias CreateTSProject="zsh ~/.zsh/script/create_ts_project.sh"

alias jupyter-notebook="~/.local/bin/jupyter-notebook --no-browser"
alias jupyter-lab="~/.local/bin/jupyter-lab --no-browser"

# git alias
alias ga="git add"
alias gm="git commit -m"
alias gp="git push"
alias gf="git fetch"
alias gr="git rebase"
